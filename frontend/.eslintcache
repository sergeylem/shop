 {' '}\n                {order.shippingAddress.country}\n              </p>\n              {order.isDelivered ? (\n                <Message variant='success'>Delivered on {order.deliveredAt}</Message>\n              ) : (\n                  <Message variant='danger'>Not Delivered</Message>\n              )}\n            </ListGroup.Item>\n\n            <ListGroup.Item>\n              <h2>Payment Method</h2>\n              <p>\n                <strong>Method:</strong> {order.paymentMethod}\n              </p>\n              {order.isPayed ? (\n                <Message variant='success'>Paid on {order.paidAt}</Message>\n              ) : (\n                  <Message variant='danger'>Not Paid</Message>\n              )}\n            </ListGroup.Item>\n\n            <ListGroup.Item>\n              <h2>Order Items</h2>\n              {order.orderItems.length === 0 ? <Message>Order is empty\n            </Message> : (\n                  <ListGroup variant='flush'>\n                    {order.orderItems.map((item, index) => (\n                      <ListGroup.Item key={index}>\n                        <Row>\n                          <Col md={1}>\n                            <Image src={item.image} alt={item.name} fluid rounded />\n                          </Col>\n                          <Col>\n                            <Link to={`/product/${item.product}`}>\n                              {item.name}\n                            </Link>\n                          </Col>\n                          <Col md={4}>\n                            {item.qty} x ${item.price} = ${item.qty * item.price}\n                          </Col>\n                        </Row>\n                      </ListGroup.Item>\n                    ))}\n                  </ListGroup>\n                )}\n            </ListGroup.Item>\n\n          </ListGroup>\n        </Col>\n        <Col md={4}>\n          <Card>\n            <ListGroup variant='flush'>\n              <ListGroup.Item>\n                <h2>Order Summary</h2>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Items</Col>\n                  <Col>${order.itemsPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n\n              <ListGroup.Item>\n                <Row>\n                  <Col>Shipping</Col>\n                  <Col>${order.shippingPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n\n              <ListGroup.Item>\n                <Row>\n                  <Col>Tax</Col>\n                  <Col>${order.taxPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n\n              <ListGroup.Item>\n                <Row>\n                  <Col>Total</Col>\n                  <Col>${order.totalPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n\n            </ListGroup>\n          </Card>\n        </Col>\n      </Row>\n    </>\n\n}\n\nexport default OrderScreen\n","/Users/sergey.lem/Documents/MyDevelop/MyPortfolio/shop/frontend/src/reducers/productReducers.js",[],"/Users/sergey.lem/Documents/MyDevelop/MyPortfolio/shop/frontend/src/constants/cartConstants.js",[],"/Users/sergey.lem/Documents/MyDevelop/MyPortfolio/shop/frontend/src/screens/UserListScreen.js",[],"/Users/sergey.lem/Documents/MyDevelop/MyPortfolio/shop/frontend/src/screens/ProductScreen.js",[],"/Users/sergey.lem/Documents/MyDevelop/MyPortfolio/shop/frontend/src/screens/UserEditScreen.js",[],"/Users/sergey.lem/Documents/MyDevelop/MyPortfolio/shop/frontend/src/screens/ProductListScreen.js",[],{"ruleId":"171","replacedBy":"172"},{"ruleId":"173","replacedBy":"174"},{"ruleId":"171","replacedBy":"175"},{"ruleId":"173","replacedBy":"176"},{"ruleId":"171","replacedBy":"177"},{"ruleId":"173","replacedBy":"178"},{"ruleId":"171","replacedBy":"179"},{"ruleId":"173","replacedBy":"180"},{"ruleId":"171","replacedBy":"181"},{"ruleId":"173","replacedBy":"182"},{"ruleId":"171","replacedBy":"183"},{"ruleId":"173","repl